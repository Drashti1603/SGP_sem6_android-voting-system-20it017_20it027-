<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:radius="20dp"
        />
    <gradient
        android:angle="45"
        android:centerX="35%"
        android:centerColor="#ff29415a"
        android:startColor="#ff000000"
        android:endColor="#ff000000"
        android:type="linear"
        />
    <padding
        android:left="8dp"
        android:top="2dp"
        android:right="8dp"
        android:bottom="2dp"
        />
    <stroke
        android:width="1dp"
        android:color="#fff1f1f1"
        />
</shape>
